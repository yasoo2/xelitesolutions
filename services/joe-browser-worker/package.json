{
  "name": "joe-browser-worker",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "ts-node-esm src/index.ts",
    "build": "esbuild src/index.ts --bundle --platform=node --format=cjs --outfile=dist/index.cjs --packages=external && esbuild src/index.ts --bundle --platform=node --format=esm --outfile=dist/index.js --packages=external",
    "start": "node dist/index.cjs",
    "install-chromium": "npx playwright install --with-deps chromium"
  },
  "dependencies": {
    "@types/ws": "^8.18.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "mime-types": "^2.1.35",
    "playwright": "^1.49.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "uuid": "^13.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.2",
    "esbuild": "^0.27.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
